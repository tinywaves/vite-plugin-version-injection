{
  "name": "vite-plugin-version-injection",
  "type": "module",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "test": "vitest run",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "simple-git-hooks",
    "prepack": "pnpm run build",
    "release": "bumpp && pnpm publish --access=public"
  },
  "peerDependencies": {
    "vite": ">=6.0.0"
  },
  "devDependencies": {
    "@dhzh/eslint-config": "^0.27.0",
    "@rslib/core": "^0.5.4",
    "@types/node": "^22.8.1",
    "bumpp": "^10.1.0",
    "eslint": "^9.21.0",
    "lint-staged": "^15.5.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vitest": "^3.0.8"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "esbuild",
      "simple-git-hooks"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
